<table>
<tr><td>
       <%=  f.label :groupName, "Group Name" %>
	</td><td>
        <%= f.text_field :groupName%>
</td></tr>
<tr><td>
       <%=  f.label :picture%>
     </td><td>
        <%= f.text_field :picture %>
</td></tr>

<tr><td>
       <%=  f.label :grouppicture, "Group Picture"%>
     </td><td>
        <%= f.file_field :grouppicture %>
</td></tr>

<tr><td>
       <%=  f.label :description %>
     </td><td>
        <%= f.text_field :description, size: 80%>
</td></tr>
<tr><td>
       <%=  f.label :email %>
     </td><td>
        <%= f.text_field :email %>
</td></tr>
<tr><td>
     <%= f.label :email_confirmation , "Email Confirmation" %>
    </td><td>
       <%= f.text_field :email_confirmation %>
</td></tr>

<tr><td>
     <%=  f.label :password %>
     </td><td>
        <%= f.password_field :password %>
</td></tr>
<tr><td>
     <%= f.label :password_confirmation , "Confirm Password " %>
    </td><td>
       <%= f.password_field :password_confirmation %>
</td></tr>
<tr><td>
    <%= f.label :groupPassword, "Choose a password for your Group :" %>
    </td><td>
     <%= f.password_field :groupPassword %>
</td></tr>
<tr><td>
     <%= f.label :groupPassword_confirmation , "Confirm Group Password " %>
    </td><td>
       <%= f.password_field :groupPassword_confirmation %>
</td></tr> 
<tr><td colspan=2 align=right ><%= f.submit "Create my Group" %></td></tr>
</table>

