<!-- This partial gets review ratings for a form named f -->
<div get_review_ratings>
<table style= "width:40%; font-size:.5em;">
   <tr>
    <td>     <%= f.label :rating %>    </td>
    <td>     <%= f.radio_button :rating, 1  %> <%= image_tag("1.png", alt: "Not Good") %> 1   </td>
    <td>     <%= f.radio_button :rating, 2 %> <%= image_tag("2.png", alt: "Loved it") %> 2    </td>
    <td>     <%= f.radio_button :rating, 3 %> <%= image_tag("3.png", alt: "Got to Read") %> 3 </td>
   </tr>
   <tr> <td> </td><td>Not Good</td><td>Loved it</td><td>Got to Read!</td></tr>
  </table>
</div>

